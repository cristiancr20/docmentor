{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "rol": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::rol.rol",
      "inversedBy": "users"
    },
    "project_ts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::new-project.new-project",
      "mappedBy": "tutor"
    },
    "project_es": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::new-project.new-project",
      "mappedBy": "estudiante"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "correccionTutor"
    },
    "notificacions": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::notificacion.notificacion",
      "mappedBy": "tutor"
    },
    "carrera": {
      "type": "string"
    },
    "itinerario": {
      "type": "enumeration",
      "enum": [
        "Ingenieria de Software",
        "Sistemas Inteligentes",
        "Compuaci√≥n Aplicada"
      ]
    }
  }
}
